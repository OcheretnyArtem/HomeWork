<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.ocheretny.homework">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homework">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homework1.HW1Activity" />
        <activity android:name=".homework2.HW2ActivityHome" />
        <activity android:name=".homework2.HW2ActivityTask1" />
        <activity android:name=".homework2.HW2ActivityTask2" />
        <activity android:name=".homework3.HW3Activity" />
        <activity android:name=".homework4.HW4Activity" />
        <activity android:name=".homework7.HW7Activity" />
    </application>

</manifest>